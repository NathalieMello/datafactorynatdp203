{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorynatdp203"
		},
		"datalakeaccountnat_LinkedServer_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeaccountnat_LinkedServer'"
		},
		"datapool_PoolActivityLog_LinkedServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'datapool_PoolActivityLog_LinkedServer'"
		},
		"datalakeaccountnat_LinkedServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeaccountnat.dfs.core.windows.net/"
		},
		"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tcp:dataworkspacenat.sql.azuresynapse.net,1433"
		},
		"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datapool"
		},
		"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
