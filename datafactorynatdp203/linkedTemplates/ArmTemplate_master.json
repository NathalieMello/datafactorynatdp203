{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorynatdp203"
		},
		"datalakeaccountnat_LinkedServer_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeaccountnat_LinkedServer'"
		},
		"datapool_PoolActivityLog_LinkedServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'datapool_PoolActivityLog_LinkedServer'"
		},
		"datalakeaccountnat_LinkedServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeaccountnat.dfs.core.windows.net/"
		},
		"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tcp:dataworkspacenat.sql.azuresynapse.net,1433"
		},
		"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datapool"
		},
		"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorynatdp203_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalakeaccountnat_LinkedServer_accountKey": {
						"value": "[parameters('datalakeaccountnat_LinkedServer_accountKey')]"
					},
					"datapool_PoolActivityLog_LinkedServer_password": {
						"value": "[parameters('datapool_PoolActivityLog_LinkedServer_password')]"
					},
					"datalakeaccountnat_LinkedServer_properties_typeProperties_url": {
						"value": "[parameters('datalakeaccountnat_LinkedServer_properties_typeProperties_url')]"
					},
					"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_server": {
						"value": "[parameters('datapool_PoolActivityLog_LinkedServer_properties_typeProperties_server')]"
					},
					"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_database": {
						"value": "[parameters('datapool_PoolActivityLog_LinkedServer_properties_typeProperties_database')]"
					},
					"datapool_PoolActivityLog_LinkedServer_properties_typeProperties_userName": {
						"value": "[parameters('datapool_PoolActivityLog_LinkedServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}